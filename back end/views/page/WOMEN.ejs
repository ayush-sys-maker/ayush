<!--WOMEN-->

<section>

  <div class="pro container-fluid justify-content-center mt-5  " style="margin-left: 100px;"  >
    <div class="row g-3"> <!-- 'g-3' adds consistent gutters -->
      <!-- Card 1 -->
       <% feature.forEach(product=>{ %>  
     <div class="ayush col-12 col-sm-6 col-md-4 col-lg-4">
  <div class="product-card h-100 p-3 border rounded"  style="cursor: pointer;">
    <img onclick="window.location.href='/products/<%= product.category %>/<%= product.id %>'" onclick="window.location.href='/products/<%= product.category %>/<%= product.id %>'" src="<%= product.image %>" class="img-fluid rounded mb-2">
    <p class="text-danger mb-1"><%= product.price %></p>
    <p class="fw-bold mb-1"><%= product.name %></p>
    <p class="text-muted small mb-1"><%= product.category %></p>
     <button  class="btn btn-dark mt-3 cart" onclick="window.location.href= '/cart/add/women/<%= product.id %>' ">ADD TO CART</button>

  </div>
</div>
      <%  } )  %>
